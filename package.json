{
  "dependencies": {
    "jest": "^27.2.2",
    "lodash": "^4.17.21",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.174",
    "@types/node": "^16.10.1",
    "@types/yargs": "^17.0.3",
    "peggy": "^1.2.0",
    "ts-node": "^10.2.1",
    "ts-pegjs": "^1.2.1",
